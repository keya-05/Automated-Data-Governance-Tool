{
  "orders": {
    "schema": {
      "order_id": {
        "type": "int",
        "required": true
      },
      "customer_name": {
        "type": "str",
        "required": true
      },
      "email": {
        "type": "str",
        "required": true
      },
      "phone": {
        "type": "str",
        "required": false
      },
      "amount": {
        "type": "float",
        "required": true,
        "min": 0
      },
      "currency": {
        "type": "str",
        "required": true,
        "allowed": [
          "INR",
          "USD",
          "GBP",
          "EUR"
        ]
      },
      "order_date": {
        "type": "date",
        "required": true,
        "format": "%Y-%m-%d"
      },
      "country": {
        "type": "str",
        "required": true
      }
    },
    "version": "0.0.1"
  },
  "enterprise_2024": {
    "schema": {
      "Industry_code_NZSIOC": {
        "type": "str",
        "required": true
      },
      "Industry_name_NZSIOC": {
        "type": "str",
        "required": true
      },
      "Units": {
        "type": "float",
        "required": false
      },
      "Year": {
        "type": "int",
        "required": true
      },
      "Variable_name": {
        "type": "str",
        "required": true
      },
      "Value": {
        "type": "float",
        "required": false
      }
    },
    "version": "0.0.1"
  },
  "large_orders": {
    "schema": {
      "order_id": {
        "type": "str",
        "required": true
      },
      "customer_name": {
        "type": "str",
        "required": true
      },
      "email": {
        "type": "str",
        "required": true
      },
      "phone": {
        "type": "str",
        "required": false
      },
      "country": {
        "type": "str",
        "required": true
      },
      "order_date": {
        "type": "date",
        "required": true,
        "format": "%Y-%m-%d"
      },
      "col_001": {
        "type": "int",
        "required": false,
        "min": 0
      },
      "col_002": {
        "type": "int",
        "required": false,
        "min": 0
      },
      "col_003": {
        "type": "int",
        "required": false,
        "min": 0
      }
    },
    "version": "0.0.1"
  }
}